{
  "name": "seven23",
  "version": "2.1.0",
  "description": "seven23",
  "scripts": {
    "start": "webpack-dev-server --config webpack-dev-server.config.js",
    "test": "npx jest --coverage",
    "test:watch": "npx jest --coverage --watch",
    "build": "webpack --config webpack-production.config.js;cp build/index.html build/404.html; cp build/index.html build/401.html",
    "build:no-progress": "webpack --config webpack-production.config.js;cp build/index.html build/404.html; cp build/index.html build/401.html",
    "serve:build": "npx http-server ./build -s -H localhost -p 8080 ",
    "backend": "docker run --env-file ./.env -p 8000:8000 -t -i sebastienbarbier/seven23",
    "format": "prettier --trailing-comma es5 --write '{src,__{tests,mocks}__}/**/*.js' --plugin=prettier-plugin-organize-imports --check",
    "format:fix": "prettier --trailing-comma es5 --write '{src,__{tests,mocks}__}/**/*.js' --plugin=prettier-plugin-organize-imports",
    "lint": "npx eslint '{src,__{tests,mocks}__}/**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "npx cypress run",
    "e2e:build": "npx cypress run --config-file ./cypress/config/cypress.production.config.js",
    "e2e:cli": "start-server-and-test serve:build http://127.0.0.1:8080 e2e:build",
    "heroku-postbuild": "npm run build",
    "predeploy": "./scripts/predeploy.sh",
    "deploy": "./scripts/deploy.sh",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/runtime": "7.28",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "cypress": "^15.5.0",
    "dotenv": "^17.2.3",
    "es6-promise": "4.2.8",
    "eslint": "^9.38.0",
    "eslint-cli": "1.1.1",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "file-loader": "^6.2.0",
    "globals": "^16.4.0",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "pretty-quick": "^4.2.2",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.6",
    "start-server-and-test": "^2.1.2",
    "style-loader": "^4.0.0",
    "stylelint": "^16.25.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "worker-loader": "^3.0.8"
