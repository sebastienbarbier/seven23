    "@mui/x-date-pickers": "^8.16.0",
    "@sentry/browser": "^10.22.0",
    "@sentry/webpack-plugin": "^4.6.0",
    "async": "^3.2.6",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.6.7",
    "babel-jest": "^30.2.0",
    "blueimp-md5": "^2.19.0",
    "browserslist": "^4.23.0",
    "caniuse-lite": "^1.0.30001751",
    "copy-webpack-plugin": "^13.0.1",
