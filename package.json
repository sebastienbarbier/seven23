    "prettier": "prettier --trailing-comma es5 --write '{src,__{tests,mocks}__}/**/*.js'",
    "e2e": "npx cypress run",
    "e2e:build": "npx cypress run --config-file cypress/config/production.json",
    "e2e:cli": "start-server-and-test serve:build http://127.0.0.1:8080 e2e:build",
    "heroku-postbuild": "npm run build"
  },
